#include<stdio.h>
int main()
{
	char ch;
	printf("enter any character");
	scanf("%c",&ch);
	
	if(ch>='0' && ch<='9')
	{
		printf("entered character is digit");
	}
	else if (ch>='A' && ch<='Z')
	{
		printf("entered character is capital letter");
	}
	else if (ch>='a' && ch<='z')
	{
		printf("entered character is small letter");
	}
	else
	{
		printf("entered character is special character");
	}
	return 0;
}
