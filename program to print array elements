#include<stdio.h>
void main()
{
    int arr[3];
    int i, j;
    printf("Enter array element :");
    for(i = 0; i < 3; i++)
    {
        scanf("%d", &arr[i]);    //Run time array initialization
    }
    printf("Array elements are : ");
    for(j = 0; j < 3; j++)
    {
        printf("%d\n", arr[j]);
    }
}
